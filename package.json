{
  "name": "vue-storage-watcher",
  "version": "1.3.1",
  "author": "dreambo8563",
  "main": "dist/vue-storage-watcher.umd.min.js",
  "browser": "dist/vue-storage-watcher.umd.min.js",
  "private": false,
  "typings": "types/main.d.ts",
  "description": "A reactive localStorage plugin for vue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "npm run lint && vue-cli-service build --target lib --name vue-storage-watcher ./src/main.ts",
    "report": "npm run lint && vue-cli-service build --report --target lib --name vue-storage-watcher ./src/main.ts",
    "test": "npm run build-bundle",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreambo8563/vue-storage-watcher.git"
  },
  "bugs": {
    "url": "https://github.com/dreambo8563/vue-storage-watcher/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-typescript": "^4.0.2",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.18.2",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10",
    "cz-conventional-changelog": "^2.1.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "types/*.d.ts",
    "package.json",
    "*.js",
    "README.md"
  ],
  "keywords": [
    "vue",
    "storage",
    "reactive",
    "vue-ls",
    "watch",
    "vue-storage"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
